<!DOCTYPE html>
<html lang="en">
<head>
  <!-- start: Meta -->
  <meta charset="utf-8">
  <title>Ecommerce</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'table', 'data-turbolinks-track': 'reload' if (params[:controller]=='orders' && params[:action]=="index") or (params[:controller]=='products' and params[:action]=='index') %>
  <%= javascript_pack_tag 'search_bar', 'data-turbolinks-track': 'reload' if params[:controller]=='orders' or (params[:controller]=='products' and params[:action]=='index') %>

  <script src="https://d3js.org/d3.v6.min.js"></script>
  <!-- end: Meta -->

  <!-- start: Mobile Specific -->
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <!-- end: Mobile Specific -->

  <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <link id="ie-style" href="css/ie.css" rel="stylesheet">
  <![endif]-->

  <!--[if IE 9]>
    <link id="ie9style" href="css/ie9.css" rel="stylesheet">
  <![endif]-->

  <!-- start: Favicon -->
  <link rel="shortcut icon" href="/favicon.ico">
  <%#= favicon_link_tag 'favicon.ico' %>
  <!-- end: Favicon -->
  <!-- Map -->
  <style>
    body {display: none; }
  </style>
  <style type="text/css">/* Chart.js */
@keyframes chartjs-render-animation{from{opacity:.99}to{opacity:1}}.chartjs-render-monitor{animation:chartjs-render-animation 1ms}.chartjs-size-monitor,.chartjs-size-monitor-expand,.chartjs-size-monitor-shrink{position:absolute;direction:ltr;left:0;top:0;right:0;bottom:0;overflow:hidden;pointer-events:none;visibility:hidden;z-index:-1}.chartjs-size-monitor-expand>div{position:absolute;width:1000000px;height:1000000px;left:0;top:0}.chartjs-size-monitor-shrink>div{position:absolute;width:200%;height:200%;left:0;top:0}</style>
<script type="text/javascript" src="chrome-extension://dbjbempljhcmhlfpfacalomonjpalpko/scripts/inspector.js"></script>
</head>

<body style="display: block;">
  <%= render "layouts/nav" %>
  <div class="main-content">
    <%= render "layouts/contant_header" unless show_header_conditions()  %>
    <div class="<%= container_fluid_class() %>">
      <%= yield %>
    </div>
  </div>
</body>
</html>